    "recharts": "^2.1.9",
    "reflect-metadata": "^0.1.13",
    "regenerator-runtime": "0.13.7",
    "rxjs": "^7.0.0",
    "tailwindcss": "^3.0.23",
    "ts-morph": "^13.0.3",
    "tslib": "^2.0.0",
    "twemoji-parser": "^14.0.0",
    "uuid": "^8.3.2"
