{
  "name": "simulate-exchange",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "run-p serve open watchers",
    "serve": "nx run-many --parallel --target=serve --all --max-parallel=100",
    "storybook:generate": "nx g @nrwl/react:stories components",
    "build": "nx run-many --parallel --target=build --prod --all",
    "test": "nx run-many --parallel --target=test --all",
    "e2e": "nx run-many --parallel --target=e2e --all",
    "prepare": "husky install",
    "watchers": "chokidar \"libs/gql/**/*.gql\" -c \"npm run generate:gql\"",
    "generate:index": "node tools/scripts/generate-index.js",
    "generate:gql": "nx run gql:generate",
    "generate:prisma": "dotenv -e .env.local npx prisma generate",
    "prisma:migrate-dev": "dotenv -e .env.local npx prisma migrate dev",
    "prisma:migrate-prod": "dotenv -e .env.production npx prisma deploy",
    "preopen": "wait-on --interval 1000 --delay 12000 --log http://localhost:4200 http://localhost:4400",
    "open": "run-p open:*",
    "open:prisma": "dotenv -e .env.local npx prisma studio",
    "open:graphql": "open-cli http://localhost:3333/graphql",
    "open:storybook": "open-cli http://localhost:4200",
    "open:next": "open-cli http://localhost:4400"
  },
  "private": true,
  "dependencies": {
    "@apollo/react-hooks": "^4.0.0",
    "@headlessui/react": "^1.5.0",
    "@heroicons/react": "^1.0.6",
    "@nestjs/apollo": "^10.0.7",
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/graphql": "^10.0.7",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/platform-ws": "^8.4.1",
    "@nestjs/websockets": "^8.4.1",
    "@nrwl/next": "13.8.5",
    "@prisma/client": "^3.10.0",
    "@types/uuid": "^8.3.4",
    "apollo-boost": "^0.4.9",
    "apollo-server-express": "^3.6.4",
    "autoprefixer": "^10.4.2",
    "chokidar-cli": "^3.0.0",
    "classnames": "^2.3.1",
    "core-js": "^3.6.5",
    "firebase": "^9.6.10",
    "firebase-admin": "^10.0.2",
    "graphql": "^15.8.0",
    "graphql-tools": "^8.2.0",
    "next": "12.1.0",
